    x:Class="TravelGlobe.MobileApp.Views.MapPage"
    Title="Mapa">
    <Grid RowDefinitions="*,Auto" Padding="0">
        <WebView x:Name="MapWebView" Source="map.html" />
        <Button Grid.Row="1" Text="Dodaj podróż" Command="{Binding AddTripCommand}" />
    </Grid>

</ContentPage>